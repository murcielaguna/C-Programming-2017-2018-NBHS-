//Displaying information through string variables
     #include <iostream>
     #include <string> 
      using namespace std;
int main()
	{
 string name = "Isabel";
 string address = "111 Bergenline Avenue, NJ 07047";
 string phone = "111-111-1111";
 
 cout << name << endl;
 cout << address << endl;
 cout << phone << endl << endl;
 return 0;
}
